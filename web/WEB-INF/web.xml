<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>edu.vt.cs5244.SimpleGame</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>webDAB</servlet-name>
        <jsp-file>/webDAB.jsp</jsp-file>
        <init-param>
            <param-name>hello</param-name>
            <param-value>test</param-value>
        </init-param>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>webDAB</servlet-name>
        <url-pattern>/webDAB.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/servlet/edu.vt.cs5244.SimpleGame</url-pattern>
    </servlet-mapping>
    
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
